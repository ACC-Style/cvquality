{
  "id": "backstop_default",
  "viewports": [
    {
      "label": "desktop",
      "width": 1200,
      "height": 1024
    }
  ],
  "onBeforeScript": "puppet/onBefore.js",
  "onReadyScript": "puppet/onReady.js",
  "scenarios": [
    {
      "label": "Atoms",
      "cookiePath": "",
      "url": "http://localhost:8000/atoms.html",
      "referenceUrl": "https://acc-style.github.io/cvquality/atoms.html",
      "readyEvent": "",
      "readySelector": ".SG-component__preview",
      "delay": 10000,
      "hideSelectors": [],
      "removeSelectors": [],
      "hoverSelector": "",
      "clickSelector": "",
      "postInteractionWait": 0,
      "selectors": [ ".SG-component__preview"  ],
      "selectorExpansion": true,
      "expect": 0,
      "misMatchThreshold": 0.1,
      "requireSameDimensions": true
    },
    {
      "label": "Molecules",
      "cookiePath": "",
      "url": "http://localhost:8000/molecules.html",
      "referenceUrl": "https://acc-style.github.io/cvquality/molecules.html",
      "readyEvent": "",
      "readySelector": ".SG-component__preview",
      "delay": 10000,
      "hideSelectors": [],
      "removeSelectors": [],
      "hoverSelector": "",
      "clickSelector": "",
      "postInteractionWait": 0,
      "selectors": [
        ".SG-component__preview"
      ],
      "selectorExpansion": true,
      "expect": 0,
      "misMatchThreshold": 0.1,
      "requireSameDimensions": true
    }, 
    {
      "label": "Structures",
      "cookiePath": "",
      "url": "http://localhost:8000/structures.html",
